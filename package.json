{
  "name": "audiebant-lockdown-solution",
  "version": "3.0.0",
  "description": "Audiebant Lockdown Solution",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "productName": "Audiebant Lockdown Solution",
    "appId": "com.audiebant-lockdown-solution.myapp",
    "asar": true,
    "mac": {
      "target": "dmg",
      "icon": "assets/img/mac-logo.icns"
    },
    "win": {
      "target": [
        {
          "target": "msi",
          "arch": [
            "ia32"
          ]
        }
      ],
      "icon": "assets/img/api_logo.png"
    },
    "linux": {
      "target": "deb",
      "icon": "assets/img/mac-logo.icns"
    },
    "directories": {
      "output": "dist"
    },
    "extraFiles": [
      {
        "from": "src/extraResources/AudiebantConfig.json",
        "to": "AudiebantConfig.json"
      }
    ]
  },
  "author": "Audiebant <info@audiebant.co.uk>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "axios": "^1.3.4",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "jquery": "^3.6.4"
  }
}
