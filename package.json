{
  "name": "communicate-and-protect",
  "version": "1.0.0",
  "description": "Communicate and Protect",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "productName": "communicate-and-protect",
    "appId": "com.communicate-and-protect.myapp",
    "asar": true,   
    "mac": {
      "target": "dmg",
       "icon":"assets/img/iconmac.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]         
        }
      ],
      "icon":"assets/img/icon-win.png"
    },
    "linux": {
      "target": "deb",
      "icon":"assets/img/icon-win.png"
     },
    "directories": {
      "output": "dist"
    }
  },
  "author": "Narola Infotech <inquiry@narolainfotech.com>",
  "license": "ISC",
  "devDependencies": {
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "jquery": "^3.6.4"
  }
}
